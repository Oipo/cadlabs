{
  "bindings": [
    {
      "webHookType": "genericJson",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "blob",
      "name": "outputBlob",
      "path": "certificates/{AttemptId}.jpg",
      "connection": "AzureWebJobsStorage",
      "direction": "out"
    }
  ]
}